{
  "name": "overamped",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "checkTypes": "tsc --noEmit",
    "build": "npm run clean && npm run build-background && npm run build-content-script && npm run build-popup",
    "build-background": "esbuild background.ts --bundle --outfile=extension/build/background.js --target=safari15",
    "build-content-script": "esbuild content-script.ts --bundle --outfile=extension/build/content-script.js --target=safari15",
    "build-popup": "rollup -c",
    "clean": "rm -rf extension/build",
    "dev-popup": "rollup -c -w",
    "check-popup": "svelte-check --tsconfig ./popup/tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier .",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephDuffy/Overamped.git"
  },
  "author": "Joseph Duffy",
  "bugs": {
    "url": "https://github.com/JosephDuffy/Overamped/issues"
  },
  "homepage": "https://github.com/JosephDuffy/Overamped#readme",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^10.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.6",
    "esbuild": "^0.15.16",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.1",
    "rollup": "^3.5.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.53.1",
    "svelte-check": "^2.10.0",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "universal-cookie": "^4.0.4",
    "web-ext-types": "^3.2.1"
  }
}
