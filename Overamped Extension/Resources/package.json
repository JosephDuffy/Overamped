{
  "name": "overamped",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "checkTypes": "tsc --noEmit",
    "build": "npm run build-background && npm run build-generic-amp-content && npm run build-google-amp-content && npm run build-google-news-article && npm run build-google-search-content && npm run build-popup && npm run build-yandex-turbo-cache",
    "build-background": "esbuild background.ts --bundle --outfile=background.js --target=safari15",
    "build-generic-amp-content": "esbuild generic-amp-content.ts --bundle --outfile=generic-amp-content.js --target=safari15",
    "build-google-amp-content": "esbuild google-amp-content.ts --bundle --outfile=google-amp-content.js --target=safari15",
    "build-google-news-article": "esbuild google-news-article.ts --bundle --outfile=google-news-article.js --target=safari15",
    "build-google-search-content": "esbuild google-search-content.ts --bundle --outfile=google-search-content.js --target=safari15",
    "build-yandex-turbo-cache": "esbuild yandex-turbo-cache.ts --bundle --outfile=yandex-turbo-cache.js --target=safari15",
    "build-popup": "rollup -c",
    "dev-popup": "rollup -c -w",
    "start-popup": "sirv popup/public --no-clear",
    "check-popup": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier .",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephDuffy/Overamped.git"
  },
  "author": "Joseph Duffy",
  "bugs": {
    "url": "https://github.com/JosephDuffy/Overamped/issues"
  },
  "homepage": "https://github.com/JosephDuffy/Overamped#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.6",
    "esbuild": "^0.12.9",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.1",
    "rollup": "^2.56.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.42.1",
    "svelte-check": "^2.2.4",
    "svelte-preprocess": "^4.7.4",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "web-ext-types": "^3.2.1"
  },
  "dependencies": {
    "sirv-cli": "^1.0.12"
  }
}
