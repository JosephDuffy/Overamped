{
  "name": "overamped",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "checkTypes": "tsc --noEmit",
    "build-google-amp-content": "esbuild google-amp-content.ts --outfile=google-amp-content.js --target=safari15",
    "build-google-search-content": "esbuild google-search-content.ts --outfile=google-search-content.js --target=safari15",
    "build-popup": "esbuild popup.ts --outfile=popup.js --target=safari15",
    "pack": "npm run build-content -- --minify",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.ts\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephDuffy/Overamped.git"
  },
  "author": "Joseph Duffy",
  "bugs": {
    "url": "https://github.com/JosephDuffy/Overamped/issues"
  },
  "homepage": "https://github.com/JosephDuffy/Overamped#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "esbuild": "^0.12.9",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4",
    "web-ext-types": "^3.2.1"
  }
}
