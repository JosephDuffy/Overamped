{
  "name": "overamped",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "checkTypes": "tsc --noEmit",
    "build": "npm run build-background && npm run build-content-script",
    "build-background": "esbuild background.ts --bundle --outfile=build/background.js --target=safari15",
    "build-content-script": "esbuild content-script.ts --bundle --outfile=build/content-script.js --target=safari15",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier .",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephDuffy/Overamped.git"
  },
  "author": "Joseph Duffy",
  "bugs": {
    "url": "https://github.com/JosephDuffy/Overamped/issues"
  },
  "homepage": "https://github.com/JosephDuffy/Overamped#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.6",
    "esbuild": "^0.15.16",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "overamped-shared": "file:../shared",
    "prettier": "^2.3.1",
    "typescript": "^4.9.3",
    "web-ext-types": "^3.2.1"
  }
}
